<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyFirstGameEngine.GamescreenController.GameScreenController">
   <HBox fx:id="backgroundBox" layoutY="-1.0" prefHeight="400.0" prefWidth="600.0" />
   <ImageView fx:id="playableCharacterSprite" fitHeight="103.0" fitWidth="149.0" focusTraversable="true" layoutX="98.0" layoutY="249.0" pickOnBounds="true" preserveRatio="true" />
   <ListView fx:id="toolSelect" prefHeight="400.0" prefWidth="70.0" />
   <ListView fx:id="underToolSelect" layoutX="69.0" layoutY="333.0" orientation="HORIZONTAL" prefHeight="67.0" prefWidth="531.0" visible="false" />


   <Button fx:id="btnConfirm" layoutX="274.0" layoutY="160.0" mnemonicParsing="false" onAction="#confirm" prefHeight="37.0" prefWidth="79.0" text="Confirm" />
   <Label fx:id="txtPoints" focusTraversable="false" layoutX="347.0" layoutY="23.0" prefHeight="19.0" prefWidth="87.0" text="Label" visible="false" />
   <Button fx:id="btnExitGame" layoutX="560.0" layoutY="10.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="25.0" prefWidth="25.0" text="x" visible="false" />
   <ImageView fx:id="enemySprite" fitHeight="89.0" fitWidth="97.0" layoutX="286.0" layoutY="256.0" pickOnBounds="true" preserveRatio="true" />
   <ImageView fx:id="carSprite" fitHeight="82.0" fitWidth="97.0" layoutX="408.0" layoutY="280.0" pickOnBounds="true" preserveRatio="true" />
   <Label fx:id="collisionTestText" layoutX="179.0" layoutY="23.0" prefHeight="51.0" prefWidth="189.0" />
   <ListView fx:id="highscoreView" focusTraversable="false" layoutX="73.0" layoutY="62.0" prefHeight="259.0" prefWidth="441.0" visible="false" />
   <Button fx:id="btnCloseHighScore" focusTraversable="false" layoutX="335.0" layoutY="280.0" mnemonicParsing="false" onAction="#closeHighScore" prefHeight="44.0" prefWidth="107.0" text="Close Highscores" visible="false" />
</AnchorPane>
